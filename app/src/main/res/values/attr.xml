<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QuickSandTextView">
        <attr name="font_style" format="enum">
            <enum name="bold" value="1"></enum>
            <enum name="medium" value="2" />
            <enum name="light" value="3" />
            <enum name="regular" value="4" />
        </attr>
        <attr name="font_size" format="integer" />
    </declare-styleable>
</resources>